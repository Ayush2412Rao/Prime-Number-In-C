#include<stdio.h>
#include<stdlib.h>
#include<conio.h>
int main()
{
	int a,i=1,b,count=0;
	printf("Enter no. to check it's prime no. or not:");
	scanf("%d",&a);
	while(1<=a)
	{
		if(a%i==0)
		{
			count=count+1;
		}
		i++;
	}
	if(count==2)
	{
		printf("It's a Prime Number");
	}
	else
	{
		printf("No it's not a Prime Number");
	}
	return 0;
}
